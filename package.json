{
  "name": "ai-readiness-assessment",
  "version": "1.0.0",
  "description": "AI Readiness Assessment Tool with Jobs-to-be-Done Framework",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:setup": "tsx scripts/setup-database.ts",
    "db:migrate": "tsx scripts/migrate-database.ts",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:reset": "tsx scripts/reset-database.ts",
    "db:status": "tsx scripts/check-database.ts",
    "supabase:types": "supabase gen types typescript --local > lib/types/database.types.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "prepare": "husky install"
  },
  "dependencies": {
