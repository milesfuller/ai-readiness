  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "ai-readiness",
    "jobs-to-be-done",
    "survey",
    "assessment",
    "nextjs",
    "supabase",
    "typescript"
